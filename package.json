{
  "name": "esy-onigurama",
  "version": "5.9.1000",
  "description": "esy-enabled build for onigurama",
  "license": "MIT",
  "esy": {
    "build": [
        ["bash", "-c", "#{os == 'windows' ? 'CC=x86_64-w64-mingw32-gcc ./configure' : './configure'}"]
    ],
    "buildsInSource": "_build",
    "exportedEnv": {
        "TREESITTER_INCLUDE_PATH": {
            "val": "#{self.install / 'include'}",
            "scope": "global"
        },
        "TREESITTER_LIB_PATH": {
            "val": "#{self.lib}",
            "scope": "global"
        }
    }
  },
  "dependencies": { }
}
